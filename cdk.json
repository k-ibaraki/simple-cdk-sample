{
  "app": "npx ts-node --prefer-ts-exts simple-cdk-sample.ts",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "node_modules"
    ]
  },
  "context": {
    "@aws-cdk/core:target-partitions": [
      "aws"
    ]
  }
}